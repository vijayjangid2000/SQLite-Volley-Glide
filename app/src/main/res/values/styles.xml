<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AlertDarkThemeCustom" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">@drawable/shape_buttons</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/whiteTextColor</item>
        <item name="android:textColorSecondary">@color/whiteTextColor</item>
        <item name="android:textColorHint">@color/whiteTextColor</item>
        <item name="android:textColorPrimary">@color/whiteTextColor</item>
        <item name="android:editTextColor">@color/whiteTextColor</item>
    </style>

</resources>